$(".rowLink").click(function () {
    // get the row containing this link 
    var row = $(this).closest("tr");
 
    // find out in which table it resides
    var table = $(this).closest("table");
 
    // move it
    row.detach();
 
    if (table.is("#table1")) {
        $("#table2").append(row);
    }
    else {
        $("#table1").append(row);
    }
 
    // draw the user's attention to it
    row.fadeOut();
    row.fadeIn();
})